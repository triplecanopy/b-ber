{
    "name": "@canopycanopycanopy/b-ber-cli",
    "version": "0.1.7",
    "description": "",
    "main": "dist/index.js",
    "bin": {
        "bber": "dist/index.js"
    },
    "scripts": {
        "prepare:dist": "BABEL_ENV=production babel --config-file ../../babel.config.js -d dist/ src/ --ignore **/__tests__/**",
        "clean": "rimraf dist",
        "prepare": "npm run clean && npm run prepare:dist",
        "build": "npm run prepare",
        "watch": "BABEL_ENV=production babel --config-file ../../babel.config.js -d dist/ src/ --watch src",
        "start": "babel-node --config-file ../../babel.config.js --trace-warnings ./src/index.js",
        "test": "jest"
    },
    "author": "Triple Canopy <b-ber@canopycanopycanopy.com> (https://triplecanopy.github.io/)",
    "license": "GPL-3.0-or-later",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@babel/runtime": "^7.5.4",
        "@canopycanopycanopy/b-ber-lib": "^0.1.7",
        "@canopycanopycanopy/b-ber-logger": "^0.1.7",
        "@canopycanopycanopy/b-ber-shapes-sequences": "^0.1.7",
        "@canopycanopycanopy/b-ber-tasks": "^0.1.7",
        "@canopycanopycanopy/b-ber-templates": "^0.1.7",
        "fs-extra": "^8.1.0",
        "lodash.has": "latest",
        "yargs": "^11.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.0",
        "@babel/core": "^7.5.4",
        "@babel/node": "^7.5.0",
        "jest": "^24.8.0",
        "rimraf": "^2.6.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/triplecanopy/b-ber.git"
    },
    "bugs": {
        "url": "https://github.com/triplecanopy/b-ber/issues"
    },
    "contributors": [
