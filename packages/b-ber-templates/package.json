        "jest": "^24.8.0",
        "rimraf": "^2.6.3"
    },
    "dependencies": {
        "@canopycanopycanopy/b-ber-lib": "^1.0.1",
        "@canopycanopycanopy/b-ber-logger": "^1.0.1",
        "fs-extra": "^8.1.0",
        "lodash.find": "latest",
        "lodash.has": "latest",
        "lodash.uniq": "latest",
        "mime-types": "^2.1.18",
